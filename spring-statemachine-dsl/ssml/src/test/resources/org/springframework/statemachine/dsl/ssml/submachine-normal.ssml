# simple 3 state sub machine with states and transitions
# defined in order and no lexical problems.

state S1 {
  initial
}

state S2 {}

state S21 {
  initial
  parent S2
}

state S22 {
  parent S2
}

state S3 {
  end
}

transition T1 {
  source S1
  target S2
  event E1
}

transition T2 {
  source S2
  target S3
  event E2
}
